        $('.rrModule .rightcolcontainerDiv div.teaserImgDesc a').textTruncate({ multiline: true, height: 30 });
        $('.rrModule .rightcolcontainerDiv div.teaserImgDesc p').textTruncate({ multiline: true, height: 50 });
    </script>

                <time id="timePrep" itemprop="prepTime" datetime="PT15M"></time>            </li>            <li id="liCook">                COOK                
                
                <span id="cookMinsSpan"><em>15</em> mins</span>
                <time id="timeCook" itemprop="cookTime" datetime="PT15M"></time>
            </li>
            <li id="liTotal" class="no-border">
                READY IN
                
                
                <span id="totalMinsSpan"><em class="emp-orange">30</em> mins</span>
                <time id="timeTotal" itemprop="totalTime" datetime="PT30M"></time>
            </li>

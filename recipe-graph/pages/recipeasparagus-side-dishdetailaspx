                <time id="timePrep" itemprop="prepTime" datetime="PT5M"></time>            </li>            <li id="liCook">                COOK                
                
                <span id="cookMinsSpan"><em>12</em> mins</span>
                <time id="timeCook" itemprop="cookTime" datetime="PT12M"></time>
            </li>
            <li id="liTotal" class="no-border">
                READY IN
                
                
                <span id="totalMinsSpan"><em class="emp-orange">17</em> mins</span>
                <time id="timeTotal" itemprop="totalTime" datetime="PT17M"></time>
            </li>

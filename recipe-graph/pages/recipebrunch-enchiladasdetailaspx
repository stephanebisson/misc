                <li id="liIngredient" data-ingredientid="1684" data-grams="7.8125">
                    <label>
                        <span class="checkbox-formatted"><input id="cbxIngredient" type="checkbox" name="ctl00$CenterColumnPlaceHolder$recipeTest$recipe$ingredients$rptIngredientsCol2$ctl03$cbxIngredient" /></span>
                        <p class="fl-ing" itemprop="ingredients">
                            <span id="lblIngAmount" class="ingredient-amount">1 tablespoon</span>
                            <span id="lblIngName" class="ingredient-name">all-purpose flour</span>
                            
                        </p>
                    </label>
                </li>
            
                <li id="liIngredient" data-ingredientid="16396" data-grams="0.6941667">

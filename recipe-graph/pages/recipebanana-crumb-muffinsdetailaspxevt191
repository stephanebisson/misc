    <script language="Javascript" type="text/javascript"><!--
var omnitureAccount="rdirdallrecipes";
// --></script><script type="text/javascript" src="http://js01.media-allrecipes.com/js/omni/v-2014.0129/ar_s_code.js"></script><script language="Javascript" type="text/javascript"><!--
function getHourOfDay(){
    var hourOfDay;
    var date=new Date();
    var hours=date.getHours();
    if (hours==12) {hourOfDay=hours + ' PM';}
    else if (hours>12) {hourOfDay=( hours-12 )+ ' PM';}
    else {hourOfDay=hours+ ' AM';}
    return hourOfDay;
}

function getDayOfWeek(){
    var retVal = '';
    switch (new Date().getDay()){
        case 0:retVal = 'Sun';break;

        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(like, s);
    });
    //+1 button
    $(function () {
        if (document.getElementById('google-plusone-sdk')) { return; }
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = '//apis.google.com/js/plusone.js'; po.id = 'google-plusone-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
    });
    //Pin It
    $(function () {
        if (document.getElementById('pin-it-sdk')) { return; }
        var pinit = document.createElement('script');
        pinit.type = 'text/javascript';
        pinit.async = true;
        var isiPad = navigator.userAgent.match(/iPad/i) != null;
        if (isiPad) {
            pinit.setAttribute("data-pin-shallow", true);
        }
        pinit.src = '//assets.pinterest.com/js/pinit.js';
        pinit.id = 'pin-it-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(pinit, s);
    });
    $(function () {
        if (document.getElementById('tweet-sdk')) { return; }
        var tweet = document.createElement('script'); tweet.type = 'text/javascript'; tweet.async = true;
        tweet.src = '//platform.twitter.com/widgets.js'; tweet.id = 'tweet-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(tweet, s);
    });
</script>

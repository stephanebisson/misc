        <div id="nutritiontable" class="nutrDetWrap" itemprop="nutrition" itemscope itemtype="http://schema.org/NutritionInformation">

            <h2>Nutritional Information</h2>
            <p class="recipeTitle"><span id="lblTitle">Individual Baked Eggs</span></p>

                <time id="timePrep" itemprop="prepTime" datetime="PT10M"></time>            </li>            
            <li id="liTotal" class="no-border">
                READY IN
                
                <span id="totalHoursSpan"><em class="emp-orange">1</em> hr</span>
                
                <time id="timeTotal" itemprop="totalTime" datetime="PT1H"></time>
            </li>

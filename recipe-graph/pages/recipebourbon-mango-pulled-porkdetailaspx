                        <span class="checkbox-formatted"><input id="cbxIngredient" type="checkbox" name="ctl00$CenterColumnPlaceHolder$recipeTest$recipe$ingredients$rptIngredientsCol2$ctl06$cbxIngredient" /></span>
                        <p class="fl-ing" itemprop="ingredients">
                            <span id="lblIngAmount" class="ingredient-amount">2 (12 ounce) bottles</span>
                            <span id="lblIngName" class="ingredient-name">barbeque sauce</span>
                            
                        </p>
                    </label>
                </li>
            
                </ul>
            

        <!-- Add class btn-disable before user checks ingredient-->
        <div id="msgAddIngredients"></div>
        <div class="shopListButtons">
            <a class="rp-btn crnrs3px grayBrdr clr-gray gray" id="btnIngredientsCheckAll" href="javascript:void(0);">Check All</a>
            <a class="linkRequiresLogin linkRequirementNextStepIsSamePage btn-disabled rp-btn crnrs3px grayBrdr clr-gray gray" id="btnIngredientsAddChecked" calltoaction="addCheckedSL Detail" href="JavaScript:void(0);" >Add to Shopping List</a>
        </div>

    </div>

    <div class="clearfix"></div>



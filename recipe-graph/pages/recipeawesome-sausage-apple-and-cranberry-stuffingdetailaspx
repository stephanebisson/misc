                <time id="timePrep" itemprop="prepTime" datetime="PT15M"></time>            </li>            <li id="liCook">                COOK                
                
                <span id="cookMinsSpan"><em>25</em> mins</span>
                <time id="timeCook" itemprop="cookTime" datetime="PT25M"></time>
            </li>
            <li id="liTotal" class="no-border">
                READY IN
                
                <span id="totalHoursSpan"><em class="emp-orange">1</em> hr</span>
                <span id="totalMinsSpan"><em class="emp-orange">40</em> mins</span>
                <time id="timeTotal" itemprop="totalTime" datetime="PT1H40M"></time>
            </li>
        </ul>

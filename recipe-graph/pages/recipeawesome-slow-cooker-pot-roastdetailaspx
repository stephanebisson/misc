                <time id="timePrep" itemprop="prepTime" datetime="PT10M"></time>            </li>            <li id="liCook">                COOK                
                <span id="cookHoursSpan"><em>8</em> hrs</span>
                
                <time id="timeCook" itemprop="cookTime" datetime="PT8H"></time>
            </li>

                <time id="timePrep" itemprop="prepTime" datetime="PT10M"></time>            </li>            <li id="liCook">                COOK                
                
                <span id="cookMinsSpan"><em>30</em> mins</span>
                <time id="timeCook" itemprop="cookTime" datetime="PT30M"></time>
            </li>
            <li id="liTotal" class="no-border">
                READY IN
                
                
                <span id="totalMinsSpan"><em class="emp-orange">40</em> mins</span>
                <time id="timeTotal" itemprop="totalTime" datetime="PT40M"></time>
            </li>

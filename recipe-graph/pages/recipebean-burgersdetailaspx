                <time id="timePrep" itemprop="prepTime" datetime="PT1H"></time>            </li>            <li id="liCook">                COOK                
                <span id="cookHoursSpan"><em>2</em> hrs</span>
                
                <time id="timeCook" itemprop="cookTime" datetime="PT2H"></time>
            </li>
            <li id="liTotal" class="no-border">
                READY IN
                
                <span id="totalHoursSpan"><em class="emp-orange">3</em> hrs</span>
                
                <time id="timeTotal" itemprop="totalTime" datetime="PT3H"></time>
            </li>

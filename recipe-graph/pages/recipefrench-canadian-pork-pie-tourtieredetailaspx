<script type="text/html" id="template_MoreLikeThisByCatItem">
    <![CDATA[
    <% if( j % 5 == 0 || j == 0 ) { %>
        <div class="carousel-item">
    <% } %>
        <ul class="fluidBucket">
            <li class="imageWrapper">
                <a id="anchorImageLink" href="<%= this.Url %>" title="<%= AR.Core.Utilities.AttributeEncode(this.Title) %>" <%if (this.Category === "Menus") { %>class=" linkRequiresSupportingMembership featureHintMenus" <% } %>>
                    <% if (this.Category === "Videos") { %>
                        <div class="videoIcon"></div>
                    <% } %>
                    <img height=70 id="imgItem" title="<%= AR.Core.Utilities.AttributeEncode(this.Title) %>" alt="<%= AR.Core.Utilities.AttributeEncode(this.Title) %>" src="<%= this.PhotoUrl %>"/>
                </a>
            </li>
            <li>
                <a href="<%= this.Url %>" id="anchorTitleLink" title="<%= AR.Core.Utilities.AttributeEncode(this.Title) %>" <%if (this.Category === "Menus") { %>class=" linkRequiresSupportingMembership featureHintMenus" <% } %>>
                    <span id="spanMoreLikeThisTitle" data-mrlt-item="title" style="visibility: visible;" ><%= this.Title %></span>
                </a>
            </li>
        </ul>
    <% if((j + 1) % 5 == 0 || j + 1 == data.length) { %>
        </div>
    <% } %>
    ]]>
</script>


</div>

                    
